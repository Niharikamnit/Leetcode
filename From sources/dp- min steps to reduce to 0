https://leetcode.com/problems/number-of-steps-to-reduce-a-number-to-zero/submissions/


 int numberOfSteps(int num) {
        int dp[1000001]={0};
        dp[0]=0;
        dp[1]=1;
        dp[2]=2;
        for(int i=3;i<=num;i++) {
            if(i%2==0) dp[i]=1+dp[i/2];
            else dp[i]=1+dp[i-1];
        }
        return dp[num];
    }
